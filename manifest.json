{
  "name": "Big blue button playback speed",
  "description": "Change big blue button playback speed!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/play-6-16.png",
      "32": "/images/play-6-32.png",
      "48": "/images/play-6-48.png",
      "128": "/images/play-6-128.png"
    }
  },
  "icons": {
    "16": "/images/play-6-16.png",
    "32": "/images/play-6-32.png",
    "48": "/images/play-6-48.png",
    "128": "/images/play-6-128.png"
  }
}
